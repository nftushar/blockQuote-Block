{
  "apiVersion": "1",
  "name": "b-blocks/blockquote",
  "title": "blockquote",
  "description": "Blockquote Gutenberg editing with easy-to-use, responsive blockquote  for  website's content",
  "category": "media",
  "keywords": ["blockquote", "block-quote", "Gutenberg"],
  "textdomain": "blockquote",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "cId": {
      "type": "string",
      "default": ""
    },
    "content": {
      "type": "string",
      "default": "Ultimate FAQ and accordion plugin create Woocommerce faq lists with comment & FAQ schema support, responsive, full customization & shortcode support."
    },
    "skin": {
      "type": "string",
      "default": ""
    },
    "background": {
      "type": "object",
      "default": {
        "color": "#ffcc00"
      }
    },
    "border": {
      "type": "object",
      "default": {
        "radius": "0px"
      }
    },
    "textAlign": {
      "type": "string",
      "default": "center"
    },
    "typography": {
      "type": "object",
      "default": {
        "fontSize": 40
      }
    },
    "quotePadding": {
      "type": "object",
      "default": {
        "top": "40px",
        "right": "40px",
        "bottom": "40px",
        "left": "40px"
      }
    },
    "color": {
      "type": "string",
      "default": "#fff"
    },
    "textShadow": {
      "type": "array",
      "default": []
    },
    "author": {
      "type": "string",
      "default": "NF Tushar"
    },
    "authorTypo": {
      "type": "object",
      "default": {
        "fontSize": 20
      }
    },
    "authorColor": {
      "type": "string",
      "default": "#336699"
    },
    "quoteColor": {
      "type": "string",
      "default": "#336699"
    },
    "quoteSize": {
      "type": "string",
      "default": "128px"
    }
  },
  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:../dist/editor.js"
}
